protocol kernel {
        persist;
        scan time 20;
        export all;
}
protocol device {
        scan time 10;
}
protocol static {
        {%- if id == 1  -%}
            route {{ tunnel_cidr }} via 10.4.5.6;
        {% endif %}
        {%- if id == 2  -%}
            route {{ tunnel_cidr }} via 10.4.6.6;
        {% endif %}
}

